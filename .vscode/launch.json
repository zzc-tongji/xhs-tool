  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "XHS Scrapor",
      "program": "${workspaceFolder}\\src\\scrape.js",
      "args": [
        "--wkdir=${workspaceFolder}\\wkdir"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "XHS Interactor",
      "program": "${workspaceFolder}\\src\\interact.js",
      "args": [
        "--wkdir=${workspaceFolder}\\wkdir",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "XHS Summary",
      "program": "${workspaceFolder}\\src\\summary.js",
      "args": [
        "--wkdir=${workspaceFolder}\\wkdir"
      ]
    }
  ]
}